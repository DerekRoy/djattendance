from django.db.models import Q

for image in uploads:
    last, first = image.name.split('_')
    Trainee.objects.filter(firstname__iexact='first')
                   .filter(lastname__iexact='last')

                   # use Q objects

                   Poll.objects.get(
                                    Q(question__startswith='Who'),
                                    Q(pub_date=date(2005, 5, 2)) | Q(pub_date=date(2005, 5, 6))
                                )

                   # get() vs. filter()?
                        # If there are no results that match the query, get() will raise a DoesNotExist exception.
                        # Similarly, Django will complain if more than one item matches the get() query. In this case, it will raise MultipleObjectsReturned, which again is an attribute of the model class itself.

